<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <RootNamespace>HTMLParser</RootNamespace>
    <StartupObject></StartupObject>
    <Company>Nersent</Company>
    <Product>HTMLParser</Product>
    <PackageId>HTMLParser</PackageId>
    <Authors>xNerhu</Authors>
    <RepositoryUrl>https://github.com/xNerhu/HTML-Parser</RepositoryUrl>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>

  <ItemGroup>
    <None Update="assets\index.html">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
